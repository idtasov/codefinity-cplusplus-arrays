#include <iostream>

void doubleArray(int arr[], int size)
{
    for (int i = 0; i<size; i++)
        {
            arr[i]= arr[i]*2;
        }
}

int main()
{
    int numbers[5] = {1, 2, 3, 4, 5};
    doubleArray(numbers, 5);
    for(int i = 0; i<5;i++)
        {
            std::cout<<numbers[i]<<std::endl;
        }
    return 0;
}