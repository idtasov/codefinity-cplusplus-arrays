#include <iostream>

// Write a function that returns the sum of all elements in the array
int sumArray(const int arr[], int size)
{
    int totalSum = 0;
    for(int i = 0; i<size; i++)
        {
            totalSum+=arr[i];
        }
    return totalSum;
}

int main()
{
    int numbers[5] = {3, 7, 2, 9, 4};
    int total = sumArray(numbers, 5);
    std::cout << total << std::endl;
    return 0;
}